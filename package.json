{
  "name": "@kloudless/file-explorer",
  "version": "1.2.0",
  "author": "Kloudless <hello@kloudless.com> (https://kloudless.com)",
  "description": "The Kloudless File Explorer is a JavaScript library that allows your users to browse and select files and folders from their storage services. It comes in two modes: a chooser and a saver.",
  "main": "./dist/loader/js/interface.js",
  "keywords": [
    "kloudless",
    "file picker",
    "file explorer",
    "cloud storage"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/kloudless/file-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/kloudless/file-explorer/issues",
    "email": "support@kloudless.com"
  },
  "homepage": "https://kloudless.com/products/file-explorer",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build:explorer && explorerUrl=http://localhost:3000/file-explorer npm run build:loader",
    "dev:explorerCss": "grunt explorerCss",
    "install-deps": "bower install",
    "build:explorer": "grunt dev -f",
    "build:explorer-only": "grunt",
    "build:explorer-deploy": "grunt deploy",
    "build:loader": "webpack --config config/webpack.loader.conf.js",
    "transpile": "babel src/loader/js -d dist/loader/js --ignore src/loader/js/webpack",
    "storybook:vue": "npm run storybook --prefix storybook-vue/",
    "storybook:react": "npm run storybook --prefix storybook-react",
    "build:storybook": "npm run build --prefix ./storybook-vue && npm run build --prefix ./storybook-react",
    "deploy:storybook": "npm run build:storybook && storybook-to-ghpages --remote=github --existing-output-dir=.demo",
    "prepack": "bash generate_npmignore.sh && npm install && npm run transpile && npm run build:loader",
    "postpack": "rm -f .npmignore"
  },
  "dependencies": {
    "hoist-non-react-statics": "3.3.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.4.1",
    "@babel/preset-react": "7.0.0",
    "@storybook/storybook-deployer": "2.8.1",
    "async": "0.9.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-stylus-compiler": "1.4.0",
    "babel-plugin-transform-define": "1.3.1",
    "bower": "1.8.8",
    "cldr-data-downloader": "0.3.5",
    "core-js": "3.1.4",
    "csso-stylus": "0.0.3",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "expose-loader": "0.7.5",
    "grunt": "0.4.5",
    "grunt-cli": "1.3.2",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "0.5.0",
    "grunt-contrib-copy": "0.5.0",
    "grunt-contrib-cssmin": "0.10.0",
    "grunt-contrib-jade": "0.12.0",
    "grunt-contrib-requirejs": "0.4.4",
    "grunt-contrib-stylus": "0.19.0",
    "grunt-contrib-uglify": "0.5.1",
    "grunt-contrib-watch": "0.6.1",
    "grunt-exec": "0.4.5",
    "grunt-includes": "0.4.5",
    "grunt-webpack": "3.1.3",
    "jade": "1.3.1",
    "jquery": "2.1.1",
    "knockout": "^3.5.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "regenerator-runtime": "0.13.2",
    "sammy": "0.7.6",
    "stylus": "0.48.1",
    "webpack": "^4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "3.7.0",
    "webpack-merge": "4.2.1"
  }
}
